{
  "expo": {
    "name": "Flockmate",
    "slug": "flockmate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#6D9773"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Flockmate needs your location to provide accurate weather data for your poultry farm."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#6D9773"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ustpjodie27.flockmate",
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "usesCleartextTraffic": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-build-properties",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Flockmate to use your location to provide accurate weather data for your poultry farm."
        }
      ]
    ],
    "extra": {
      "supabaseUrl": "https://mizksqfqtwmgwnxjevqi.supabase.co",
      "supabaseAnonKey": "sb_publishable_oimeq0CBdRM2lctsOIYUeQ_uYDfTHMo"
    }
  }
}
